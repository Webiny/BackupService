{
    "name": "webiny/backup-service",
    "type": "library",
    "description": "Simple library to do backups and store them to AWS S3",
    "keywords": [
        "backup",
        "amazon s3"
    ],
    "homepage": "http://www.webiny.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Al Hamad",
            "email": "sven@webiny.com"
        },
        {
            "name": "Pavel Denisjuk",
            "email": "pavel@webiny.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "webiny/std-lib": "1.5",
        "webiny/config": "1.5",
        "webiny/amazon": "1.5",
        "wp-cli/php-cli-tools": "^0.11.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "mybuilder/phpunit-accelerator": "dev-master",
        "webiny/amazon": "~1"
    },
    "autoload": {
        "psr-4": {
            "Webiny\\BackupService\\": "src/Webiny/BackupService/"
        }
    },
    "extras": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    }
}
